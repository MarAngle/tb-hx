  align-items: center;
  width: 750rpx;
}

.page-pay-success-menu-item{
  width: auto;
  background-color: rgba(245,134,11,1);
  color: #fff;
  padding: 20rpx 30rpx;
  border-radius: 12rpx;
  margin: 0 30rpx;
}
