.detail {
  padding: 14rpx 14rpx 30rpx;
  background-color: #F5F5F5;
  min-height: 100vh;
  box-sizing: border-box;
}

.state {
  background-color: #ffffff;
  padding: 20rpx 40rpx;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
  color: #494949;
}

.state .order-no {
  display: flex;
  justify-content: space-between;
  border-bottom: 1rpx solid #bbbbbb;
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}

.order-no > view:first-of-type {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  font-weight: 600;
}

.order-no .icon {
  margin-left: 10rpx;
}

.order-no > view:last-of-type {
  font-size: 26rpx;
}

.state .order-state {
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}

.state .order-state::before {
  content: '';
  display: block;
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background-color: #9ADA51;
  margin: 0 10rpx;
}

.order-state .title {
  margin-right: auto;
  font-size: 28rpx;
}

.order-state view:nth-last-of-type(2) {
  font-size: 24rpx;
  color: #8B8B8C;
}

.state .state-detail {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 24rpx;
  background-color: #f5f5f5;
  padding: 10rpx 20rpx;
  border-radius: 40rpx;
  margin-bottom: 20rpx;
}

.state-detail view:last-of-type {
  color: #F39B11;
  border: 1rpx solid #F39B11;
  padding: 6rpx 20rpx;
  border-radius: 20rpx;
  font-size: 22rpx;
}

.state .state-info > view {
  display: flex;
  font-size: 26rpx;
  margin-top: 20rpx;
}

.state .state-info > view > view:first-of-type {
  font-weight: 600;
}

