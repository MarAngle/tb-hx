  height: 100vh;
  padding-top: 0rpx;
  background-color: #f7f7f7;
}

.page-pay-success-icon image{
  width: 650rpx;
  height: auto;
  margin: 0rpx auto 0rpx auto;
}

.page-pay-success-menu{
  margin-top: -320rpx;
  display: flex;
